@{
    ViewBag.Title = "Home Page";

    string FloodreCertificateName = System.Configuration.ConfigurationManager.AppSettings["floodre_certificate_name"].ToString();
    string Demo = System.Configuration.ConfigurationManager.AppSettings["Demo"].ToString();
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
    <div class="col-md-4">
        <h2>Floodre Certificate Name</h2>
        <p>
            @FloodreCertificateName
        </p>        
    </div>
    <div class="col-md-4">
        <h2>Demo</h2>
        <p>@Demo</p>        
    </div>    
</div>